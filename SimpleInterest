public class SimpleInterest {
    public static void main(String[] args) {

        double p, r, t, si; // 4 box  are created
        p = 100; // principle
        r = 10; // rate
        t = 2;  // time
        si = (p * r * t) / 100;
        System.out.println("The simple interest of given values is " +si);
    }
}
